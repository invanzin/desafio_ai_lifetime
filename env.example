# ============================================================================
# VARIÁVEIS DE AMBIENTE - Meeting Extractor API
# ============================================================================
# Copie este arquivo para .env: copy env.example .env
# Depois preencha com suas credenciais reais


# ============================================================================
# OPENAI
# ============================================================================

# Sua chave da OpenAI (obtenha em: https://platform.openai.com/api-keys)
OPENAI_API_KEY=sk-proj-your-key-here

# Modelo a usar (padrão: gpt-4o)
OPENAI_MODEL=gpt-4o


# ============================================================================
# LANGCHAIN HUB
# ============================================================================

# Nome do prompt no LangChain Hub
EXTRACTOR_PROMPT_HUB_NAME=ivan-furukawa/extrator-reunioes-bancarias


# ============================================================================
# RATE LIMITING
# ============================================================================

# Máximo de requisições por minuto por IP (padrão: 10)
RATE_LIMIT_PER_MINUTE=10


# ============================================================================
# LANGSMITH - Observabilidade (OPCIONAL)
# ============================================================================

# Ativar tracing? (true/false)
LANGCHAIN_TRACING_V2=true

# Chave da API LangSmith (obtenha em: https://smith.langchain.com/settings)
LANGCHAIN_API_KEY=ls__your-key-here

# Nome do projeto no LangSmith
LANGCHAIN_PROJECT=meeting-api






